<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.calculator0ffood.ui.history.HistoryFragment"
        android:label="title_home"
        tools:layout="@layout/fragment_today" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.calculator0ffood.ui.today.TodayFragment"
        android:label="title_dashboard"
        tools:layout="@layout/fragment_history" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.calculator0ffood.ui.profile.ProfileFragment"
        android:label="title_notifications"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/navigation_eat"
        android:name="com.example.calculator0ffood.ui.eat.EatFragment"
        android:label="title_eat"
        tools:layout="@layout/fragment_eat"
        />
</navigation>